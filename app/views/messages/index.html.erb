<ul>
<% @messages.each do |message| %>
  <li><%= message.text %></li>
<% end %>
</ul>

<p><%= link_to 'New message', new_messages_path %></p>


<p><%= link_to 'New text', texts_new_path %></p>

<p><%= Time.now %></p>
<% if @send_in %>
<p> <%=@send_in %> </p>
<% end %>

<% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
